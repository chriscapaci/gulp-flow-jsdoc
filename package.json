{
    "name": "gulp-flow-jsdoc",
    "version": "0.3.0",
    "description": "Gulp plugin for flow-jsdoc package to convert jsdoc comments to flow types",
    "license": "MIT",
    "keywords": [
        "gulpplugin",
        "flow-jsdoc"
    ],
    "homepage": "https://github.com/chriscapaci/gulp-flow-jsdoc",
    "bugs": "https://github.com/chriscapaci/gulp-flow-jsdoc/issues",
    "author": {
        "name": "Chris Capaci",
        "email": "chris.capaci@gmail.com",
        "url": "https://github.com/chriscapaci"
    },
    "main": "./lib/index.js",
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/chriscapaci/gulp-flow-jsdoc.git"
    },
    "dependencies": {
        "flow-jsdoc": "0.3.1",
        "plugin-error": "1.0.1"
    },
    "devDependencies": {
        "mocha": "5.0.1",
        "eslint": "4.19.1",
        "vinyl": "2.1.0",
        "gulp": "3.9.1",
        "gulp-eslint": "4.0.2",
        "gulp-clean": "0.4.0",
        "gulp-mocha": "5.0.0",
        "gulp-rename": "1.2.2"
    },
    "scripts": {
        "test": "node_modules/.bin/gulp test",
        "lint": "node_modules/.bin/gulp lint"
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 6,
            "impliedStrict": true
        },
        "rules": {
            "indent": [
                "error",
                4,
                {
                    "SwitchCase": 1
                }
            ],
            "curly": "error",
            "brace-style": [
                "error",
                "1tbs"
            ],
            "semi": [
                "error",
                "always"
            ],
            "no-extra-semi": "error",
            "no-unreachable": "error",
            "eqeqeq": "error",
            "default-case": "error",
            "no-alert": "error",
            "no-unused-labels": "error"
        }
    }
}
